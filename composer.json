{
    "name": "innmind/git-release",
    "type": "library",
    "description": "Tool to help release new packages versions",
    "keywords": ["git", "release", "tool"],
    "homepage": "http://github.com/Innmind/GitRelease",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/GitRelease/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/foundation": "~2.1",
        "innmind/git": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\GitRelease\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\GitRelease\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "~1.3",
        "innmind/black-box": "~6.5",
        "innmind/coding-standard": "~2.0"
    },
    "bin": ["git-release"]
}
